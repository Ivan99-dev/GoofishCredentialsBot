# 环境变量示例文件
# 复制此文件为 .env 并填入实际值

# Node 环境
NODE_ENV=production

# 服务端口
PORT=3000

# OpenAI API 配置（如果使用）
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_BASE_URL=https://api.openai.com/v1

# API 安全密钥（如果使用）
# API_SECRET=your_secret_key_here

# 数据库路径
# DB_PATH=./data/goofishcbot.db

# 日志级别
# LOG_LEVEL=info
